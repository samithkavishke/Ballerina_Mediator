<template xmlns="http://ws.apache.org/ns/synapse" name="{{name}}">
    {{#each params ~}}
    <parameter name="{{name}}" description="{{description}}"/>
    {{/each ~}}
    <sequence>
        {{#each params ~}}
        <property name="{{name}}"  expression="$func:{{name}}"/>
        {{/each ~}}
        <class name="{{class}}"/>
    </sequence>
</template>
